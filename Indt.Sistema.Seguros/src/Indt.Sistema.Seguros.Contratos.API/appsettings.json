{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=127.0.0.1; initial Catalog=IDTSeguro; User ID=sa; password=techchanllenge@123;Trusted_Connection=False;Trust Server Certificate=true;",
    "Prefix": "idtsistemaseguro"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Destructure": [
      {
        "Name": "ToMaximumDepth",
        "Args": { "maximumDestructuringDepth": 4 }
      },
      {
        "Name": "ToMaximumStringLength",
        "Args": { "maximumStringLength": 100 }
      },
      {
        "Name": "ToMaximumCollectionCount",
        "Args": { "maximumCollectionCount": 10 }
      }
    ],
    "Properties": {
      "Application": "ContatoApp"
    }
  },
  "RabbitmqServiceSettings": {
    "User": "guest",
    "Password": "guest",
    "Retry": {
      "RetryLimit": 10,
      "MinimumInterval": 60,
      "MaximumInteval": 300
    },
    "ContatoEvent": {
      "ConnectionString": "rabbitmq://rabbitmq",
      "ConcurrencyLimit": 30,
      "OperationTimeout": 20,
      "Queues": {
        "ContatoAtualizadoV1": {
          "Name": "v1-contato-atualizado",
          "PrefetchCount": 10
        },
        "ContatoCriadoV1": {
          "Name": "v1-contato-criado",
          "PrefetchCount": 10
        },
        "ContatoDeletadoV1": {
          "Name": "v1-contato-deletado",
          "PrefetchCount": 10
        }
      }
    }
  },
  "AllowedHosts": "*"
}